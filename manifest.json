{
  "manifest_version": 3,
  "name": "Convert WYSIWYG to Markdown",
  "action": {},
  "version": "1.0.0.0",
  "description": "extention for hypothesis",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "scripts/content/content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "scripts/background/background.js"
  }
}
